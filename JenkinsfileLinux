pipeline {
    agent { label 'cking168' }
    // tools { maven 'M3' }
    stages {
        stage('Init') {
            steps {
              checkout scm
              script {
                showBuildDesc()
              }
            }
        }
        stage('Build') {
            steps {
                sh 'mvn clean package'
            }
        }
        stage('Docker Image') {
            steps {
                script {
                    def dockerImage = docker.build("my-app:latest")
                }
            }
        }
    }
}
